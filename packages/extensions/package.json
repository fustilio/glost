{
  "name": "glost-extensions",
  "version": "0.1.1",
  "description": "Core extension system and built-in extensions for GLOST",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./processor": {
      "types": "./src/processor.ts",
      "default": "./src/processor.ts"
    },
    "./registry": {
      "types": "./src/registry.ts",
      "default": "./src/registry.ts"
    },
    "./extensions": {
      "types": "./src/extensions/index.ts",
      "default": "./src/extensions/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "lint": "echo 'No linting configured'"
  },
  "dependencies": {
    "glost": "workspace:*",
    "glost-common": "workspace:*",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "vitest": "^3.0.5"
  },
  "keywords": [
    "glost",
    "extensions",
    "plugins",
    "transformation",
    "metadata"
  ]
}
